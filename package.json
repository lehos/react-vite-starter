{
  "name": "react-vite-starter",
  "version": "0.0.0",
  "private": true,
  "author": "alexey.stratan@gmail.com",
  "scripts": {
    "dev": "vite",
    "dev-hash": "VITE_HISTORY_HASH=true vite",
    "build": "vite build",
    "build:gh": "vite build --base=/react-vite-starter/",
    "serve": "vite preview",
    "ts": "tsc",
    "format": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "predeploy": "cross-env VITE_HISTORY_HASH=true ASSET_PATH=/react-vite-starter/ yarn build:gh",
    "deploy": "gh-pages -d dist --remote origin",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "antd": "4.18.5",
    "final-form": "4.20.6",
    "final-form-arrays": "^3.0.2",
    "history": "4.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-final-form": "6.5.7",
    "react-final-form-arrays": "^3.1.3",
    "react-query": "3.34.12",
    "react-router-dom": "5.3.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.7.2",
    "@types/node": "17.0.13",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/parser": "5.10.1",
    "@vitejs/plugin-react": "1.1.4",
    "eslint": "8.8.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.2",
    "prettier": "2.5.1",
    "typescript": "4.5.5",
    "vite": "2.7.13"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
