{
  "name": "react-vite-starter",
  "version": "0.0.0",
  "private": true,
  "author": "alexey.stratan@gmail.com",
  "scripts": {
    "dev": "vite",
    "dev-hash": "VITE_HISTORY_HASH=true vite",
    "build": "vite build",
    "build:gh": "vite build --base=/react-vite-starter/",
    "serve": "vite preview",
    "ts": "tsc",
    "format": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "predeploy": "cross-env VITE_HISTORY_HASH=true ASSET_PATH=/react-vite-starter/ yarn build:gh",
    "deploy": "gh-pages -d dist --remote origin"
  },
  "dependencies": {
    "@ant-design/icons": "4.6.2",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "antd": "4.16.6",
    "final-form": "4.20.2",
    "final-form-arrays": "^3.0.2",
    "history": "4.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-final-form": "6.5.3",
    "react-final-form-arrays": "^3.1.3",
    "react-query": "3.18.1",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@types/node": "15.12.5",
    "@types/react": "17.0.12",
    "@types/react-dom": "17.0.8",
    "@types/react-router-dom": "5.1.7",
    "@vitejs/plugin-react-refresh": "1.3.4",
    "prettier": "2.3.2",
    "typescript": "4.3.5",
    "vite": "2.3.8"
  }
}
